{
  "manifest_version": 3,
  "name": "node-modules utils",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.npmjs.com/package/*"],
      "js": ["src/content.jsx"]
    }
  ],

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
